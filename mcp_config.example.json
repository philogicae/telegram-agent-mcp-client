{
  "mcpServers": {
    "sequential-thinking": {
      "command": "uvx sequential-thinking-mcp",
      "disabled": false
    },
    "web-search": {
      "command": "uvx duckduckgo-mcp-server",
      "edit": {
        "search": {
          "name": "web_search",
          "description": "Search on the web and return formatted results"
        },
        "fetch_content": {
          "name": "fetch_https_url",
          "description": "Fetch and parse content from a webpage URL always starting with https://"
        }
      },
      "disabled": false
    },
    "news-search": {
      "command": "npx -y @modelcontextprotocol/server-brave-search",
      "edit": {
        "brave_web_search": {
          "description": "Search on the web using the Brave Search API, ideal for news and articles. Use this for broad information gathering and recent events."
        },
        "brave_local_search": false
      },
      "env": {
        "BRAVE_API_KEY": "<add-your-key>"
      },
      "disabled": true
    },
    "torrent-search": {
      "serverUrl": "http://localhost:8765/sse",
      "edit": {
        "get_torrent_info": false
      },
      "disabled": true
    },
    "torrent-client": {
      "command": "uvx rqbit-mcp",
      "env": {
        "RQBIT_URL": "http://localhost:3030"
      },
      "edit": {
        "start_torrent": false,
        "pause_torrent": false,
        "delete_torrent": false,
        "get_torrent_details": false
      },
      "disabled": true
    }
  }
}
