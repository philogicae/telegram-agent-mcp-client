{
  "sequential-thinking": {
    "command": "uvx",
    "args": ["sequential-thinking-mcp"],
    "transport": "stdio"
  },
  "web-search": {
    "command": "uvx",
    "args": ["duckduckgo-mcp-server"],
    "transport": "stdio"
  },
  "torrent-search": {
    "command": "uvx",
    "args": ["torrent-search-mcp"],
    "transport": "stdio"
  },
  "torrent-client": {
    "command": "uvx",
    "args": ["rqbit-mcp"],
    "env": { "RQBIT_URL": "http://localhost:3030" },
    "transport": "stdio"
  }
}
